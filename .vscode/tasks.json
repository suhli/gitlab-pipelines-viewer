{
  "version": "2.0.0",
  "tasks": [
    {
      // 一次性编译任务，跑完就结束
      "label": "npm: compile",
      "type": "npm",
      "script": "compile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
